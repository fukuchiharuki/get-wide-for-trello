{
  "manifest_version": 3,
  "name": "Get Wide for Trello",
  "description": "Show trello cards wide.",
  "version": "1.4.0",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "./icon-128x128.png"
  },
  "icons": {
    "128": "icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "all_frames": false,
      "css": ["override.css"]
    }
  ]
}
